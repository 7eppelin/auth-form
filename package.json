{
    "name": "auth-form",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint:types": "tsc --noEmit",
        "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path ./.gitignore",
        "lint:eslint": "yarn eslint ./src --ext .jsx,.js,.ts,.tsx --ignore-path ./.gitignore",
        "lint": "yarn unused-exports && yarn lint:types && yarn lint:eslint",
        "prepare": "husky install",
        "unused-exports": "node --require tsm scripts/unused-exports.ts"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "styled-components": "5.3.5",
        "zustand": "4.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.0.3",
        "@commitlint/config-conventional": "17.0.3",
        "@types/node": "18.7.3",
        "@types/react": "18.0.15",
        "@types/react-dom": "18.0.6",
        "@types/styled-components": "5.1.26",
        "@typescript-eslint/eslint-plugin": "5.33.0",
        "@typescript-eslint/parser": "5.33.0",
        "@vitejs/plugin-react": "2.0.0",
        "eslint": "8.21.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.30.1",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "husky": "8.0.0",
        "lint-staged": "12.4.3",
        "prettier": "2.7.1",
        "ts-unused-exports": "8.0.0",
        "tsm": "2.2.2",
        "typescript": "4.6.4",
        "vite": "3.0.0"
    },
    "resolutions": {
        "styled-components": "^5"
    }
}
